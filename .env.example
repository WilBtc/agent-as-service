# AaaS Configuration

# API Configuration
HOST=0.0.0.0
PORT=8000

# Claude Code Configuration
CLAUDE_CODE_PATH=claude
ANTHROPIC_API_KEY=your-api-key-here
CLAUDE_MODEL=claude-sonnet-4-5-20250929

# Agent Configuration
MAX_AGENTS=100
AGENT_TIMEOUT=3600
DEFAULT_WORKING_DIR=/tmp/aaas-agents

# Storage
DATA_DIR=./data
LOGS_DIR=./logs

# Security
API_KEY_HEADER=X-API-Key
ALLOWED_ORIGINS=*

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
